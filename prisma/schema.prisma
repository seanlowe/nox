datasource database {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Meal {
  id       Int       @id @default(autoincrement())
  name     String
  type     String
  lastMade DateTime?
}

model Server {
  id   Int    @id @default(autoincrement())
  name String
  ipv4 String
  port String
}
